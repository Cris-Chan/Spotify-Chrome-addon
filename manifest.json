{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["resources/jquery-3.4.1.min.js", "scripts/content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "resources/play.png",
    "default_popup": "interface/popup.html"
  }
}
